{
  "private": true,
  "name": "@govuk-frontend/review",
  "description": "GOV.UK Frontend review app",
  "main": "src/start.mjs",
  "engines": {
    "node": "^20.9.0",
    "npm": "^10.1.0"
  },
  "license": "MIT",
  "scripts": {
    "dev": "gulp dev --color",
    "prebuild": "npm run clean",
    "build": "gulp build --color",
    "build:sassdoc": "sassdoc --config sassdoc.config.yaml ../idsk-frontend/src/govuk",
    "build:types": "tsc --build tsconfig.build.json",
    "build:jsdoc": "typedoc",
    "postbuild": "npm run build:jsdoc && npm run build:sassdoc",
    "clean": "del-cli dist",
    "proxy": "browser-sync start --config browsersync.config.js",
    "serve": "nodemon",
    "start": "node src/start.mjs",
    "test:screenshots": "gulp screenshots --color"
  },
  "dependencies": {
    "@govuk-frontend/config": "*",
    "@govuk-frontend/lib": "*",
    "@govuk-frontend/stats": "*",
    "@prettier/sync": "^0.5.1",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "front-matter": "^4.0.2",
    "govuk-frontend": "*",
    "highlight.js": "^11.9.0",
    "iframe-resizer": "^4.3.9",
    "marked": "^12.0.0",
    "marked-linkify-it": "^3.1.9",
    "marked-smartypants": "^1.1.6",
    "outdent": "^0.8.0",
    "shuffle-seed": "^1.1.6",
    "slash": "^3.0.0",
    "slug": "^8.2.3"
  },
  "devDependencies": {
    "@govuk-frontend/helpers": "*",
    "@govuk-frontend/tasks": "*",
    "@percy/cli": "^1.28.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "autoprefixer": "^10.4.17",
    "browser-sync": "^3.0.2",
    "cssnano": "^6.0.3",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "nodemon": "^3.0.3",
    "postcss-pseudo-classes": "^0.4.0",
    "puppeteer": "^22.1.0",
    "rollup": "^4.12.0",
    "sassdoc": "^2.7.4",
    "supertest": "^6.3.4",
    "typedoc": "^0.25.8"
  },
  "optionalDependencies": {
    "nunjucks": "^3.2.4"
  }
}
